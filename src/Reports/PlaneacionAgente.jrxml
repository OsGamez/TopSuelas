	<queryString>
		<![CDATA[SELECT 
 DISTINCT a.Descripcion as Agente,DATEPART(WK,p.Fecha_Entrega) as Fecha
FROM Pedidos p 
INNER JOIN Dpedido dp 
ON p.Npedido = dp.Npedido
INNER JOIN CobranzaPhy.dbo.Clientes c
ON dp.Id_Cliente = c.Id_Cliente
INNER JOIN CobranzaPhy.dbo.Agentes a
ON c.Id_Agente = a.Id_Agente
WHERE DATEPART(WK,p.Fecha_Entrega) = $P{Date}
GROUP BY dp.Pares,p.Fecha_Entrega,a.Descripcion]]>
	</queryString>
