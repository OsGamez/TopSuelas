	<queryString>
		<![CDATA[SELECT 
 p.Fecha_Entrega,c.Id_Cliente,c.RazonSocial,p.Npedido
,
CASE
	WHEN p.Fecha_Entrega BETWEEN '2018-12-31' AND '2019-01-05' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-01-07' AND '2019-01-12' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-01-14' AND '2019-01-19' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-01-21' AND '2019-01-26' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-01-28' AND '2019-02-02' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-02-04' AND '2019-02-09' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-02-11' AND '2019-02-16' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-02-18' AND '2019-02-23' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-02-25' AND '2019-03-02' THEN p.TotalPares	 
	WHEN p.Fecha_Entrega BETWEEN '2019-03-04' AND '2019-03-09' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-03-11' AND '2019-03-16' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-03-18' AND '2019-03-23' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-03-25' AND '2019-03-30' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-04-01' AND '2019-04-06' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-04-08' AND '2019-04-13' THEN p.TotalPares
	WHEN p.Fecha_Entrega BETWEEN '2019-04-15' AND '2019-04-20' THEN p.TotalPares
	END AS Pares
FROM Pedidos p INNER JOIN CobranzaPhy.dbo.Clientes c
ON p.Id_Cliente = c.Id_Cliente
WHERE DATEPART(WK,p.Fecha_Entrega) = $P{Date} 
ORDER BY p.Npedido]]>
	</queryString>
