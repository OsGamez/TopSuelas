	<queryString>
		<![CDATA[SELECT 
 DISTINCT c.Descripcion as Color,DATEPART(WK,p.Fecha_Entrega) as Fecha
FROM Pedidos p 
INNER JOIN Dpedido dp 
ON p.Npedido = dp.Npedido
INNER JOIN ProduccionPhy.dbo.Producto pd
ON dp.Id_Producto = pd.Id_Producto
INNER JOIN ProduccionPhy.dbo.Color c
ON pd.Id_Color = c.Id_Color
WHERE DATEPART(WK,p.Fecha_Entrega) = $P{Date}
GROUP BY dp.Pares,p.Fecha_Entrega,c.Descripcion]]>
	</queryString>
	<field name="Color" class="java.lang.String"/>
	<field name="Fecha" class="java.lang.Integer"/>
