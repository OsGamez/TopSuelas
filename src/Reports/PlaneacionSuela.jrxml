	<queryString>
		<![CDATA[SELECT 
 DISTINCT l.Descripcion as Modelo,DATEPART(WK,p.Fecha_Entrega) as Fecha
FROM Pedidos p 
INNER JOIN Dpedido dp 
ON p.Npedido = dp.Npedido
INNER JOIN ProduccionPhy.dbo.Producto pd
ON dp.Id_Producto = pd.Id_Producto
INNER JOIN ProduccionPhy.dbo.Linea l
ON pd.Id_Linea = l.Id_Linea
WHERE DATEPART(WK,p.Fecha_Entrega) = $P{Date}
GROUP BY dp.Pares,p.Fecha_Entrega,l.Descripcion]]>
	</queryString>
	<field name="Modelo" class="java.lang.String"/>
	<field name="Fecha" class="java.lang.Integer"/>
