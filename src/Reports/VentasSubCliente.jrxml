	<queryString>
		<![CDATA[select c.NumCliente,c.Nombre,sum(f.TotalPares) as Pares,sum(f.Total) as Total FROM 
Clientes c
INNER JOIN RCPTPhylon..Facturas f
ON c.NumCliente = f.NumCliente 
WHERE f.CveAgente = $P!{cve}
GROUP BY c.NumCliente, c.Nombre
order by Total desc]]>
	</queryString>
