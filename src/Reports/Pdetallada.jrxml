 WHEN DATEPART(WK,Fecha_Entrega) = 36 THEN '02/09/2019' + '  -  ' + '07/09/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 37 THEN '09/09/2019' + '  -  ' + '14/09/2019'
