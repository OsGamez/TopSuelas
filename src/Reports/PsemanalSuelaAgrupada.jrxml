 WHEN DATEPART(WK,Fecha_Entrega) = 31 THEN '29/07/2019' + '  -  ' + '03/08/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 32 THEN '05/08/2019' + '  -  ' + '10/08/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 33 THEN '12/08/2019' + '  -  ' + '17/08/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 34 THEN '19/08/2019' + '  -  ' + '24/08/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 35 THEN '26/08/2019' + '  -  ' + '31/08/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 36 THEN '02/09/2019' + '  -  ' + '07/09/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 37 THEN '09/09/2019' + '  -  ' + '14/09/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 38 THEN '16/09/2019' + '  -  ' + '21/09/2019'
