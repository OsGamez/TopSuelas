 WHEN DATEPART(WK,Fecha_Entrega) = 43 THEN '21/10/2019' + '  -  ' + '26/10/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 44 THEN '28/10/2019' + '  -  ' + '02/11/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 45 THEN '04/11/2019' + '  -  ' + '09/11/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 46 THEN '11/11/2019' + '  -  ' + '16/11/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 47 THEN '18/11/2019' + '  -  ' + '23/11/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 48 THEN '25/11/2019' + '  -  ' + '30/11/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 49 THEN '02/12/2019' + '  -  ' + '07/12/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 50 THEN '09/12/2019' + '  -  ' + '14/12/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 51 THEN '16/12/2019' + '  -  ' + '21/12/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 52 THEN '23/12/2019' + '  -  ' + '28/12/2019'
 WHEN DATEPART(WK,Fecha_Entrega) = 53 THEN '30/12/2019' + '  -  ' + '04/01/2020'
 END AS 'RANGO'
FROM Pedidos 
WHERE Fecha_Entrega BETWEEN $P{fecha} AND $P{fecha2}
GROUP BY DATEPART(WK,Fecha_Entrega)]]>
	</queryString>
